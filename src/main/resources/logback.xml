<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="FILE" class="ch.qos.logback.core.FileAppender">
        <file>/Users/lanliwz/downloads/j2d.log</file>
        <append>true</append>
        <encoder>
            <pattern>%date{MM/dd HH:mm:ss} %-5level[%.15thread] %logger{1} - %msg%n</pattern>
        </encoder>
    </appender>
    <logger name="slick.backend.DatabaseComponent.action" level="debug"/>
    <logger name="slick.jdbc" level="debug" />
    <logger name="slick.ast" level="debug" />
    <logger name="slick.memory" level="info" />
    <logger name="slick.relational" level="info" />
    <logger name="slick.compiler" level="info" />
    <logger name="scala" level="info" />
    <logger name="utils" level="debug" />
    <root level="debug">
        <appender-ref ref="FILE"/>
    </root>

</configuration>
